{
    "version": "0.1.0",
    "adapters": {
      "boulevard": {
        "enabled": true,
        "mapping_file": "config/adapters/boulevard_mappings.json"
      }
    },
    "input": {
      "directory": "data/input"
    },
    "output": {
      "directory": "data/output",
      "format": "json"
    },
    "validation": {
      "strict": false,
      "report_file": "validation_report.json"
    },
    "logging": {
      "level": "INFO",
      "file": "logs/processing.log"
    }
  }